{
  "name": "@exploreui/wc-elements",
  "version": "0.0.0",
  "description": "List of framework agnostic web components",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn esbuild && yarn ts:declarations",
    "esbuild": "node scripts/build.cjs",
    "esbuild:watch": "node scripts/build.cjs --dev",
    "serve": "concurrently -k -r \"yarn esbuild:watch\" \"wds\"",
    "ts:declarations": "tsc --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anandrikka/exploreui-webcomponents.git"
  },
  "keywords": [
    "web",
    "components",
    "custom",
    "components",
    "lit",
    "lit-html"
  ],
  "author": "Anand Reddy Rikka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anandrikka/exploreui-webcomponents/issues"
  },
  "homepage": "https://github.com/anandrikka/exploreui-webcomponents#readme",
  "devDependencies": {
    "@web/dev-server": "^0.1.17",
    "concurrently": "^6.2.0",
    "esbuild": "^0.12.8",
    "globby": "^11.0.3"
  },
  "dependencies": {
    "@exploreui/wc-core": "0.0.0"
  },
  "exports": {
    ".": "./index.js"
  }
}
