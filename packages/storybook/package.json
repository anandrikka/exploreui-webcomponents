  "dependencies": {
    "@exploreui/lit": "^0.0.1-alpha.1",
    "@exploreui/lit-hello-world": "^0.0.1-alpha.2",
    "lit-html": "^1.4.1"
  },
  "devDependencies": {
    "@web/dev-server-storybook": "^0.3.5"
  }
}
